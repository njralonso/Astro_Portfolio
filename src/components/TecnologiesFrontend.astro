---
import { Image } from "astro:assets";
import Tecnologies from "./Tecnologies.astro";
import IconJavascript from "../images/icons/javascript.svg";
import IconTypescript from "../images/icons/typescript.svg";
import IconReact from "../images/icons/react.svg";
import IconAstro from "../images/icons/astro.svg";
import IconJquery from "../images/icons/jquery.svg";
import IconTailwind from "../images/icons/tailwindcss.svg";
import IconBootstrap from "../images/icons/bootstrap.svg";
import IconSass from "../images/icons/sass.svg";
import "../styles/app.css";
---

<Tecnologies title="Frontend">
	<Image
		src={IconJavascript}
		alt="icon_javascript.svg"
		class="w-12 h-12 tech-icon"
	/>
	<Image
		src={IconTypescript}
		alt="icon_typescript.svg"
		class="w-12 h-12 tech-icon"
	/>
	<Image src={IconReact} alt="icon_react.svg" class="w-12 h-12 tech-icon" />
	<Image src={IconAstro} alt="icon_astro.svg" class="w-12 h-12 tech-icon" />
	<Image src={IconJquery} alt="icon_jquery.svg" class="w-12 h-12 tech-icon" />
	<Image
		src={IconTailwind}
		alt="icon_tailwindcss.svg"
		class="w-12 h-12 tech-icon"
	/>
	<Image
		src={IconBootstrap}
		alt="icon_bootstrap.svg"
		class="w-12 h-12 tech-icon"
	/>
	<Image src={IconSass} alt="icon_sass.svg" class="w-12 h-12 tech-icon" />
</Tecnologies>
