---
import { Image } from "astro:assets";
import Tecnologies from "./Tecnologies.astro";
import IconJavascript from "../images/icons/javascript.svg";
import IconTypescript from "../images/icons/typescript.svg";
import IconReact from "../images/icons/react.svg";
import IconAstro from "../images/icons/astro.svg";
import IconJquery from "../images/icons/jquery.svg";
import IconTailwind from "../images/icons/tailwindcss.svg";
import IconBootstrap from "../images/icons/bootstrap.svg";
import IconSass from "../images/icons/sass.svg";
---

<Tecnologies title="Frontend">
	<Image src={IconJavascript} alt="icon_javascript.svg" class="w-12 h-12" />
	<Image src={IconTypescript} alt="icon_typescript.svg" class="w-12 h-12" />
	<Image src={IconReact} alt="icon_react.svg" class="w-12 h-12" />
	<Image src={IconAstro} alt="icon_astro.svg" class="w-12 h-12" />
	<Image src={IconJquery} alt="icon_jquery.svg" class="w-12 h-12" />
	<Image src={IconTailwind} alt="icon_tailwindcss.svg" class="w-12 h-12" />
	<Image src={IconBootstrap} alt="icon_bootstrap.svg" class="w-12 h-12" />
	<Image src={IconSass} alt="icon_sass.svg" class="w-12 h-12" />
</Tecnologies>
