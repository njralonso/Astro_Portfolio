---
import { Image } from "astro:assets";
import Tecnologies from "./Tecnologies.astro";

import IconPhp from "../images/icons/php.svg";
import IconLaravel from "../images/icons/laravel.svg";
import Livewire from "../images/icons/livewire.svg";
---

<Tecnologies title="Backend">
	<Image src={IconPhp} alt="icon_php.svg" class="w-12 h-12" />
	<Image src={IconLaravel} alt="icon_laravel.svg" class="w-12 h-12" />
	<Image src={Livewire} alt="icon_livewire.svg" class="w-12 h-12" />
</Tecnologies>
