---
import { Image } from "astro:assets";
import Tecnologies from "./Tecnologies.astro";

import IconPhp from "../images/icons/php.svg";
import IconLaravel from "../images/icons/laravel.svg";
import Mysql from "../images/icons/mysql.svg";
import MongoDB from "../images/icons/mongodb.svg";
import Express from "../images/icons/icons8-express-js.svg";
import Nodejs from "../images/icons/nodejs.svg";
---

<Tecnologies title="Backend">
	<Image src={IconPhp} alt="icon_php.svg" class="w-12 h-12 tech-icon" />
	<Image src={IconLaravel} alt="icon_laravel.svg" class="w-12 h-12 tech-icon" />
	<Image src={Express} alt="icon_express.svg" class="w-12 h-12 tech-icon" />
	<Image src={Mysql} alt="icon_mysql.svg" class="w-12 h-12 tech-icon" />
	<Image src={MongoDB} alt="icon_mongodb.svg" class="w-12 h-12 tech-icon" />
	<Image src={Nodejs} alt="icon_nodejs.svg" class="w-12 h-12 tech-icon" />
</Tecnologies>
